{
  "name": "docstring-cli",
  "description": "Easily add docstrings via a structured format or let local AI do the job",
  "version": "0.1.0",
  "main": "dist/index.js",
  "bin": {
    "docstring": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "templates/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "version:prerelease": "npm version prerelease",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:prerelease": "node scripts/release.js prerelease",
    "release": "node scripts/release.js"
  },
  "keywords": [
    "docstring",
    "cli",
    "typescript",
    "developer-tools"
  ],
  "author": "Remco Stoeten <remcostoeten@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/remcostoeten/docstring-cli.git"
  },
  "homepage": "https://github.com/remcostoeten/docstring-cli#readme",
  "bugs": {
    "url": "https://github.com/remcostoeten/docstring-cli/issues"
  },
  "dependencies": {
    "enquirer": "^2.4.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
